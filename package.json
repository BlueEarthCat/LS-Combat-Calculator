{
  "name": "ls_combat-calculator",
  "productName": "Last Saviors Combat Calculator",
  "version": "1.0.3",
  "description": "For Minecraft 1.21.4 Server Last Saviors",
  "main": "main.js",
  "build": {
    "appId": "com.last-saviors.combat-calculator",
    "productName": "Last Saviors Combat Calculator",
    "files": [
      "index.html",
      "main.js",
      "preload.js",
      "images/**",
      "package.json"
    ],
    "directories": {
      "output": "dist"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "blueearthcat",
        "repo": "LS-Combat-Calculator"
      }
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64"]
        }
      ],
      "icon": "images/galaxia.ico"
    },
    "linux": {
      "target": [
        {
          "target": "deb",
          "arch": ["x64"]
        }
      ]
    }
  },
  "scripts": {
    "start": "electron .",
    "build:win": "electron-builder --win nsis --publish never",
    "build:mac": "electron-builder --mac dmg --publish never",
    "build:linux": "electron-builder --linux AppImage deb --publish never"
  },
  "author": {
    "name": "BlueEarthCat",
    "email": "doheun94@gmail.com"
  },
  "license": "ISC",
  "devDependencies": {
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "electron-installer-windows": "^3.0.0",
    "@electron/packager": "^18.4.4",
    "electron-updater": "^6.6.2",
    "rimraf": "^5.0.10",
    "glob": "^10.4.5",
    "@npmcli/fs": "^3.1.1",
    "minimatch": "^9.0.5"
  },
  "dependencies": {
    "boolean": "^3.2.0",
    "buffer-crc32": "^0.2.13",
    "cacheable-lookup": "^5.0.4",
    "cacheable-request": "^7.0.4",
    "clone-response": "^1.0.3",
    "debug": "^4.4.3",
    "decompress-response": "^6.0.0",
    "defer-to-connect": "^2.0.1",
    "define-data-property": "^1.1.4",
    "define-properties": "^1.2.1",
    "detect-node": "^2.1.0",
    "end-of-stream": "^1.4.5",
    "env-paths": "^2.2.1",
    "es-define-property": "^1.0.1",
    "es-errors": "^1.3.0",
    "es6-error": "^4.1.1",
    "escape-string-regexp": "^4.0.0",
    "extract-zip": "^2.0.1",
    "fd-slicer": "^1.1.0",
    "fs-extra": "^8.1.0",
    "get-stream": "^5.2.0",
    "glob": "^11.0.3",
    "global-agent": "^3.0.0",
    "globalthis": "^1.0.4",
    "gopd": "^1.2.0",
    "got": "^11.8.6",
    "graceful-fs": "^4.2.11",
    "has-property-descriptors": "^1.0.2",
    "http-cache-semantics": "^4.2.0",
    "http2-wrapper": "^1.0.3",
    "json-buffer": "^3.0.1",
    "json-stringify-safe": "^5.0.1",
    "jsonfile": "^4.0.0",
    "keyv": "^4.5.4",
    "lowercase-keys": "^2.0.0",
    "lru-cache": "^11.2.2",
    "matcher": "^3.0.0",
    "mimic-response": "^1.0.1",
    "ms": "^2.1.3",
    "normalize-url": "^6.1.0",
    "object-keys": "^1.1.1",
    "once": "^1.4.0",
    "p-cancelable": "^2.1.1",
    "pend": "^1.2.0",
    "progress": "^2.0.3",
    "pump": "^3.0.3",
    "quick-lru": "^5.1.1",
    "resolve-alpn": "^1.2.1",
